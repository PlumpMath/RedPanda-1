          <label>Specular Color</label>
          <object class="sizeritem">
            <option>1</option>
            <flag>wxEXPAND</flag>
            <object class="wxFlexGridSizer">
              <hgap>0</hgap>
              <rows>4</rows>
              <cols>3</cols>
              <vgap>0</vgap>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Intensity</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxSlider" name="slSpecI">
